@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

################################################################################
# Base classes

:Entity a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Entity"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf owl:Thing, rdfs:Resource .

################################################################################
# Top-level classes

:Birthday a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Birthday"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity .

:Bookmark a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Bookmark"@en ;
  rdfs:comment "A record of the address of a web page serving as a shortcut to it."@en ;
  rdfs:subClassOf :Entity .

:Note a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Note"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity .

:Paper a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Paper"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity .

:Payment a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Payment"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity .

:Person a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Person"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity, foaf:Person .

:Place a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Place"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity .

:Project a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Project"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity, doap:Project, foaf:Project .

:Quote a rdfs:Class ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Quote"@en ;
  rdfs:comment ""@en ;
  rdfs:subClassOf :Entity .

